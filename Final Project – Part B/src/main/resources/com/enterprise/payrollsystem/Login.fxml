<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="238.0" prefWidth="341.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.enterprise.payrollsystem.LoginController">
   <children>
      <Label layoutX="100.0" layoutY="30.0" text="Username" />
      <TextField fx:id="usernameField" layoutX="100.0" layoutY="50.0" />
      <Label layoutX="100.0" layoutY="90.0" text="Password" />
      <PasswordField fx:id="passwordField" layoutX="100.0" layoutY="110.0" />
      <Button layoutX="100.0" layoutY="150.0" onAction="#login" text="Login" />
      <Button layoutX="100.0" layoutY="180.0" onAction="#navigateToRegister" text="Register" />
   </children>
</AnchorPane>
